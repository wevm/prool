{
  "name": "prool",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && tsc --project ./tsconfig.build.json",
    "changeset": "changeset",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm install --lockfile-only && pnpm format",
    "check": "biome check .",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf src/_lib src/tsconfig.build.tsbuildinfo",
    "test": "vitest --testTimeout 10000",
    "test:cov": "vitest --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@pimlico/alto": "0.0.15",
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^24.10.2",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^1.6.0",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0",
    "ws": "^8.18.3"
  },
  "packageManager": "pnpm@10.25.0"
}
